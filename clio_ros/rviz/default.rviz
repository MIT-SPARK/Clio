Panels:
  - Class: rviz/Displays
    Help Height: 0
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /Active Window1
      Splitter Ratio: 0.5
    Tree Height: 650
  - Class: rviz/Selection
    Name: Selection
  - Class: rviz/Tool Properties
    Expanded:
      - /2D Pose Estimate1
      - /2D Nav Goal1
      - /Publish Point1
    Name: Tool Properties
    Splitter Ratio: 0.5886790156364441
  - Class: rviz/Views
    Expanded:
      - /Current View1
    Name: Views
    Splitter Ratio: 0.5
  - Class: rviz/Time
    Name: Time
    SyncMode: 0
    SyncSource: Segmentation
Preferences:
  PromptSaveOnExit: true
Toolbars:
  toolButtonStyle: 2
Visualization Manager:
  Class: ""
  Displays:
    - Class: rviz/TF
      Enabled: true
      Filter (blacklist): khronos_object
      Filter (whitelist): ""
      Frame Timeout: 15
      Frames:
        All Enabled: true
        dominic/forward_link:
          Value: true
        world:
          Value: true
      Marker Alpha: 1
      Marker Scale: 1
      Name: TF
      Show Arrows: true
      Show Axes: true
      Show Names: true
      Tree:
        world:
          dominic/forward_link:
            {}
      Update Interval: 0
      Value: true
    - Class: rviz/MarkerArray
      Enabled: true
      Marker Topic: /clio_visualizer/dsg_markers
      Name: Scene Graph
      Namespaces:
        "dynamic_edges_\x00": true
        "dynamic_label_\x00": true
        "dynamic_nodes_\x00": true
        interlayer_edges_3_1: true
        layer_bounding_boxes_1: true
        layer_edges_3: true
        layer_nodes_1: true
        layer_nodes_3: true
      Queue Size: 100
      Value: true
    - Class: kimera_pgmo_rviz/MeshDisplay
      Cull Backfaces: true
      Enable Lighting: false
      Enabled: true
      Name: Background
      Queue Size: 10
      Toggle Visibility All: true
      Topic: /clio_visualizer/dsg_mesh
      Unreliable: false
      Value: true
      Visible:
        Value: true
        robot0:
          Value: true
          dsg_mesh: true
    - Class: kimera_pgmo_rviz/MeshDisplay
      Cull Backfaces: false
      Enable Lighting: false
      Enabled: true
      Name: Object Meshes
      Queue Size: 10
      Toggle Visibility All: true
      Topic: /clio_visualizer/khronos_objects/static_objects
      Unreliable: false
      Value: true
      Visible: true
    - Class: rviz/Group
      Displays:
        - Class: kimera_pgmo_rviz/MeshDisplay
          Cull Backfaces: true
          Enable Lighting: false
          Enabled: false
          Name: Active Mesh
          Queue Size: 10
          Toggle Visibility All: true
          Topic: /clio_node/reconstruction/mesh
          Unreliable: false
          Value: false
          Visible: true
        - Class: rviz/Image
          Enabled: true
          Image Topic: /clio_node/reconstruction/camera/labels
          Max Value: 1
          Median window: 5
          Min Value: 0
          Name: Segmentation
          Normalize Range: true
          Queue Size: 2
          Transport Hint: raw
          Unreliable: false
          Value: true
      Enabled: true
      Name: Active Window
  Enabled: true
  Global Options:
    Background Color: 255; 255; 255
    Default Light: true
    Fixed Frame: world
    Frame Rate: 30
  Name: root
  Tools:
    - Class: rviz/Interact
      Hide Inactive Objects: true
    - Class: rviz/MoveCamera
    - Class: rviz/Select
    - Class: rviz/FocusCamera
    - Class: rviz/Measure
    - Class: rviz/SetInitialPose
      Theta std deviation: 0.2617993950843811
      Topic: /initialpose
      X std deviation: 0.5
      Y std deviation: 0.5
    - Class: rviz/SetGoal
      Topic: /move_base_simple/goal
    - Class: rviz/PublishPoint
      Single click: true
      Topic: /clicked_point
  Value: true
  Views:
    Current:
      Class: rviz/Orbit
      Distance: 22.5767879486084
      Enable Stereo Rendering:
        Stereo Eye Separation: 0.05999999865889549
        Stereo Focal Distance: 1
        Swap Stereo Eyes: false
        Value: false
      Field of View: 0.7853981852531433
      Focal Point:
        X: -4.554806232452393
        Y: 5.374069690704346
        Z: -1.520478367805481
      Focal Shape Fixed Size: false
      Focal Shape Size: 0.05000000074505806
      Invert Z Axis: false
      Name: Current View
      Near Clip Distance: 0.009999999776482582
      Pitch: 0.49479618668556213
      Target Frame: dominic/forward_link
      Yaw: 5.04360294342041
    Saved: ~
